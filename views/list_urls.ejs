<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>URL Shortener - URLs</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
            margin-bottom: 20px;
        }
        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h1>URL Shortener - URLs</h1>
    <table>
        <tr>
            <th>Short ID</th>
            <th>Redirect URL</th>
        </tr>
        <% urls.forEach(function(url) { %>
            <tr>
                <td><%= url
                    .shortId %></td>
                <td><a href="<%= url.redirectUrl %>"><%= url.redirectUrl %></a></td>
            </tr>
        <% }); %>
    </table>
</body>
        </html>
            